{
  "name": "@judgeq/server",
  "version": "0.0.43",
  "packageManager": "pnpm@7.1.6",
  "description": "JudgeQ API Server",
  "repository": "https://github.com/JudgeQ-Dev/server",
  "author": "Dup4",
  "license": "AGPL-3.0",
  "engines": {
    "node": ">=16.11.0"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "dev": "cross-env JUDGEQ_LOG_SQL=1 JUDGEQ_CONFIG_FILE=./config.yaml nest start --debug --watch",
    "start": "node dist/main",
    "build": "rimraf dist && nest build && bash post_build.sh",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint ./src/**/*.ts",
    "lint:style": "prettier --check src/",
    "format": "run-p format:*",
    "format:style": "prettier --write src/",
    "test": "vitest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register --files ./node_modules/typeorm/cli -f ./ormconfig.js"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.5",
    "@nestjs/core": "^8.4.5",
    "@nestjs/platform-express": "^8.4.5",
    "@nestjs/platform-socket.io": "^8.4.5",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.4",
    "@nestjs/websockets": "^8.4.5",
    "@nestlab/google-recaptcha": "^2.1.2",
    "bcryptjs": "^2.4.3",
    "caught": "^0.1.3",
    "cheerio": "^1.0.0-rc.11",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "ejs": "^3.1.8",
    "emoji-regex": "^10.1.0",
    "express": "4.18.1",
    "fs-extra": "^9.1.0",
    "git-repo-info": "^2.1.1",
    "highlight.js": "10.5.0",
    "ioredis": "^4.28.5",
    "ip2region": "^2.3.0",
    "js-yaml": "^4.1.0",
    "jsondiffpatch": "^0.4.1",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^3.0.0",
    "minio": "^7.0.28",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.5",
    "object-path": "^0.11.8",
    "patch-packages": "^1.0.0",
    "postinstall-postinstall": "^2.1.0",
    "proxy-agent": "^4.0.1",
    "randomstring": "^1.2.2",
    "rate-limiter-flexible": "^2.3.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.1.0",
    "serialize-javascript": "^5.0.1",
    "socket.io-msgpack-parser": "^2.2.1",
    "socket.io-redis": "^6.1.1",
    "streamsaver": "^2.0.6",
    "swagger-ui-express": "^4.4.0",
    "telegraf": "^4.8.2",
    "tempy": "^1.0.1",
    "terser": "^5.13.1",
    "toposort": "^2.0.2",
    "typeorm": "0.2.31",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.2",
    "xdomain": "^0.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cheerio": "^0.22.31",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^27.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/minio": "^7.0.13",
    "@types/node": "^17.0.36",
    "@types/nodemailer": "^6.4.4",
    "@types/object-path": "^0.11.1",
    "@types/randomstring": "^1.1.8",
    "@types/serialize-javascript": "^5.0.2",
    "@types/supertest": "^2.0.12",
    "@types/toposort": "^2.0.3",
    "@types/unzipper": "^0.10.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "pnpm": "^7.1.6",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.2",
    "vitest": "^0.12.9",
    "webpack": "^5.72.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
