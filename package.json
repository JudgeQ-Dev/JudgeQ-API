{
  "name": "hznuoj-v3-api",
  "version": "0.0.32",
  "description": "The 3th generation HZNUOJ API Server",
  "repository": "https://github.com/HZNU-OJ/HZNUOJ-V3-API",
  "author": "Dup4",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check-style": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env HZNUOJ_V3_LOG_SQL=1 HZNUOJ_V3_CONFIG_FILE=./config.yaml nest start --watch",
    "start:debug": "cross-env HZNUOJ_V3_LOG_SQL=1 HZNUOJ_V3_CONFIG_FILE=./config.yaml nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "gitHook": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^7.6.6",
    "@nestjs/core": "^7.6.6",
    "@nestjs/platform-express": "^7.6.6",
    "@nestjs/platform-socket.io": "^7.6.13",
    "@nestjs/swagger": "^4.7.15",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.13",
    "@nestlab/google-recaptcha": "^1.2.4",
    "bcryptjs": "^2.4.3",
    "caught": "^0.1.3",
    "cheerio": "^1.0.0-rc.5",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "ejs": "^3.1.6",
    "fs-extra": "^9.1.0",
    "git-repo-info": "^2.1.1",
    "ioredis": "^4.23.0",
    "ip2region": "^2.1.1",
    "js-yaml": "^4.0.0",
    "jsondiffpatch": "^0.4.1",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.3",
    "minio": "^7.0.18",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.5.0",
    "object-path": "^0.11.5",
    "randomstring": "^1.1.5",
    "rate-limiter-flexible": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "serialize-javascript": "^5.0.1",
    "socket.io-msgpack-parser": "^3.0.1",
    "socket.io-redis": "^6.0.1",
    "streamsaver": "^2.0.5",
    "swagger-ui-express": "^4.1.6",
    "terser": "^5.6.0",
    "toposort": "^2.0.2",
    "typeorm": "^0.2.31",
    "unzipper": "^0.10.11",
    "xdomain": "^0.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cheerio": "^0.22.28",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.8",
    "@types/fs-extra": "^9.0.7",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^26.0.15",
    "@types/js-yaml": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/minio": "^7.0.7",
    "@types/node": "^14.14.6",
    "@types/nodemailer": "^6.4.0",
    "@types/object-path": "^0.11.0",
    "@types/randomstring": "^1.1.6",
    "@types/serialize-javascript": "^5.0.0",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-redis": "^1.0.27",
    "@types/supertest": "^2.0.10",
    "@types/toposort": "^2.0.3",
    "@types/unzipper": "^0.10.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "yorkie": "^2.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=14.15.5"
  }
}
